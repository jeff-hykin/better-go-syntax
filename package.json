{
  "name": "better-go-syntax",
  "displayName": "Better Go Syntax",
  "description": "A syntax improvement for go lang",
  "icon": "icon.png",
  "version": "1.0.10",
  "scripts": {
    "build": "ruby source/generate.rb",
    "publish": "npm run build && vsce publish",
    "postinstall": "bundle install"
  },
  "keywords": [
    "syntax",
    "textmate",
    "highlighting",
    "coloring",
    "color",
    "go"
  ],
  "engines": {
    "vscode": "^1.0.0"
  },
  "author": {
    "name": "Jeff Hykin"
  },
  "publisher": "jeff-hykin",
  "contributes": {
    "languages": [
      {
        "id": "go (simplified)",
        "aliases": [
          "Go (simplified)",
          "go (simplified)"
        ],
        "extensions": [
          ".go_simplified"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "go",
        "scopeName": "source.go",
        "path": "./export/generated.tmLanguage.json"
      },
      {
        "language": "go (simplified)",
        "scopeName": "source.go_simplified",
        "path": "./export/simplified.tmLanguage.json"
      }
    ]
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "url": "https://github.com/jeff-hykin/cpp-textmate-grammer",
    "type": "git"
  },
  "devDependencies": {
    "chalk": "^2.4.2",
    "convert-svg-to-png": "^0.5.0",
    "dictionary-en-us": "^2.1.1",
    "glob": "^7.1.4",
    "js-yaml": "^3.13.1",
    "json-source-map": "^0.4.0",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.19",
    "minimatch": "^3.0.4",
    "node-gyp": "^4.0.0",
    "nspell": "^2.1.2",
    "oniguruma": "^7.2.0",
    "prettyjson": "^1.2.1",
    "vscode-textmate": "^4.1.1",
    "yargs": "^13.2.2"
  },
  "dependencies": {}
}
